{
  "name": "test",
  "private": true,
  "version": "0.4.2",
  "type": "module",
  "scripts": {
    "prepare": "rm -rf .husky && husky install && husky add .husky/pre-commit \"npx lint-staged\" && husky add .husky/pre-push \"npm run ci:deploy\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "ci:init": "mb-ci init",
    "ci:deploy": "mb-ci deploy"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^5.2.2",
    "axios": "^1.3.3",
    "classnames": "^2.3.2",
    "less": "^4.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1",
    "socket.io-client": "^4.6.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.34.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mb-ci": "^0.1.1",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.3"
  },
  "lint-staged": {
    "**/*.{ts,tsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
